---
pagination:
  data: languages
  size: 1
  alias: data
  addAllPagesToCollections: true
permalink: "{{ data.lang.path }}index.html"
---
{%- assign lang = data.lang -%}
{%- assign localize = data.localize -%}
{%- assign title = data.localize.home.title -%}

<!doctype html>
<html lang="{{ lang.symbol }}">
{% include head %}
<body>
  <h1>{{ title }}</h1>
  <p>{{ localize.home.description }}</p>
</body>
</html>